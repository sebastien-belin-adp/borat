<?xml version="1.0" encoding="UTF-8"?><standard-workflow>
          <consumer class="fs-consumer">
            <unique-id>Channel-5671058</unique-id>
            <poller class="fixed-interval-poller">
              <poll-interval>
                <unit>SECONDS</unit>
                <interval>10</interval>
              </poll-interval>
            </poller>
            <create-dirs>true</create-dirs>
            <file-sorter class="fs-sort-none"/>
            <base-directory-url>${consumeDestination}</base-directory-url>
            <wip-suffix>.wip</wip-suffix>
          </consumer>
          <service-collection class="service-list">
            <unique-id>ServiceList-9080760</unique-id>
            <services>
              <add-metadata-service>
                <unique-id>backstabbing-morse</unique-id>
                <metadata-element>
                  <key>key</key>
                  <value>${metadataValue}</value>
                </metadata-element>
              </add-metadata-service>
              <add-timestamp-metadata-service>
                <unique-id>sad-fermat</unique-id>
                <metadata-key>timestamp</metadata-key>
                <date-format-builder>
                  <format>yyyy-MM-dd'T'HH:mm:ssZ</format>
                </date-format-builder>
              </add-timestamp-metadata-service>
            </services>
          </service-collection>
          <producer class="fs-producer">
            <unique-id>FS-Producer</unique-id>
            <create-dirs>true</create-dirs>
            <fs-worker class="fs-nio-worker"/>
            <filename-creator class="formatted-filename-creator">
              <filename-format>%2$tQ.xml</filename-format>
            </filename-creator>
            <destination class="configured-produce-destination">
              <destination>${produceDest}</destination>
            </destination>
          </producer>
          <produce-exception-handler class="null-produce-exception-handler"/>
          <unique-id>Workflow3</unique-id>
          <message-metrics-interceptor>
            <unique-id>Workflow3-MessageMetrics</unique-id>
            <timeslice-duration>
              <unit>MINUTES</unit>
              <interval>5</interval>
            </timeslice-duration>
            <timeslice-history-count>12</timeslice-history-count>
          </message-metrics-interceptor>
          <in-flight-workflow-interceptor>
            <unique-id>Workflow3-InFlight</unique-id>
          </in-flight-workflow-interceptor>
        </standard-workflow>